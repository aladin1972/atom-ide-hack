{
  "repository": "https://github.com/hhvm/atom-ide-hack",
  "enhancedScopes": [
    "source.hack",
    "text.html.hack"
  ],
  "author": "Facebook Inc.",
  "description": "atom-ide integration for Hack",
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    }
  },
  "name": "ide-hack",
  "dependencies": {
    "atom-languageclient": "^0.9.5"
  },
  "main": "index.js"
}
